node {
  stage('SCM') {
    git ' https://github.com/shiwanee-code/my-app.git'
  }
 stage('Compile-Package'){
      // Get maven home path
       
      sh "mvn package"
   }
   
   stage('SonarQube Analysis') {
        withSonarQubeEnv('sonar-6') { 
          sh "mvn sonar:sonar"
        }
    }
  }

